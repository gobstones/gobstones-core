{
    "entryPoints": [
        "./src"
    ],
    "entryPointStrategy": "expand",
    "out": "docs",
    "includes": "./src",
    "exclude": [
        "**/*.test.ts",
        "./src/index.ts"
    ],
    "includeVersion": true,
    "categorizeByGroup": true,
    "excludeExternals": true,
    "excludeInternal": false,
    "excludePrivate": false,
    "hideGenerator": true,
    "disableSources": true,
    "githubPages": true,
    "mergeModulesMergeMode": "module"
}
